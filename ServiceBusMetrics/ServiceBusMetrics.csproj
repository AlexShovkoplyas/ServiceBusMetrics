    <TargetFramework>netcoreapp2.2</TargetFramework>
    <LangVersion>7.3</LangVersion>
    <RootNamespace>ServiceBusMetrics</RootNamespace>
    <AssemblyName>ServiceBusMetrics</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Autofac.Extras.IocManager.DynamicProxy" Version="3.2.2" />
    <PackageReference Include="ConsoleTableExt" Version="2.0.2" />
